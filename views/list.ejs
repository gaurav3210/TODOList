<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>


 <div class="box">
   <%  newListItems.forEach(function (itemPresent) { %>
    <div class="item">
      <input type="checkbox">
          <p><%= itemPresent.name %></p>
    </div>
   <%  }) %>
   <form action="/" method="post">
     <label for="NextItem"></label>
     <input type="text" name="newItem" placeholder="New Item", autocomplete="off">
     <button type="submit" name="Submit", value=<%= listTitle %> >+</button>
   </form>
 </div>

<%- include("footer") -%>
